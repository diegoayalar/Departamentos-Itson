<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>AgregarDepartamento</servlet-name>
        <servlet-class>servlet.AgregarDepartamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarDepartamento</servlet-name>
        <servlet-class>servlet.ActualizarDepartamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarDepartamento</servlet-name>
        <servlet-class>servlet.EliminarDepartamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarInquilino</servlet-name>
        <servlet-class>servlet.AgregarInquilino</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarInquilino</servlet-name>
        <servlet-class>servlet.ActualizarInquilino</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DarBajaInquilino</servlet-name>
        <servlet-class>servlet.DarBajaInquilino</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarPago</servlet-name>
        <servlet-class>servlet.AgregarPago</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarPago</servlet-name>
        <servlet-class>servlet.ActualizarPago</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PonerPendientePago</servlet-name>
        <servlet-class>servlet.PonerPendientePago</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IniciarSesion</servlet-name>
        <servlet-class>servlet.IniciarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarUsuario</servlet-name>
        <servlet-class>servlet.RegistrarUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AgregarDepartamento</servlet-name>
        <url-pattern>/agregarDepartamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarDepartamento</servlet-name>
        <url-pattern>/actualizarDepartamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarDepartamento</servlet-name>
        <url-pattern>/eliminarDepartamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarInquilino</servlet-name>
        <url-pattern>/agregarInquilino</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarInquilino</servlet-name>
        <url-pattern>/actualizarInquilino</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DarBajaInquilino</servlet-name>
        <url-pattern>/darBajaInquilino</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarPago</servlet-name>
        <url-pattern>/agregarPago</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarPago</servlet-name>
        <url-pattern>/actualizarPago</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PonerPendientePago</servlet-name>
        <url-pattern>/ponerPendientePago</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IniciarSesion</servlet-name>
        <url-pattern>/iniciarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarUsuario</servlet-name>
        <url-pattern>/registrarUsuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
